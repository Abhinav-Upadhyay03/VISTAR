{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"npm:electron-dev\"",
    "electron-dev": "cross-env NODE_ENV=development wait-on http://localhost:5173 && electron .",
    "build": "cross-env NODE_ENV=production vite build && electron-builder",
    "start": "electron ."
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.7",
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.510.0",
    "react": "^18.3.1",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.3.1",
    "react-image-crop": "^11.0.7",
    "react-router-dom": "^6.28.0",
    "react-tooltip": "^5.28.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "vite": "^5.4.10",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.example.tsdl",
    "productName": "TSDL Project",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "electron.js",
      "index.html",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
